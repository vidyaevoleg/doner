					<div class="lo-r-cont-review-head-user-img anon"  ng-show="review.anonym"></div>
					<div class="lo-r-cont-review-head-user-info">
						<div class="lo-r-cont-review-head-user-info-name" ng-hide="review.anonym"><a ng-href="{{review.author.link}}" target="_blank">{{review.author.username}}</a></div>
						<div class="lo-r-cont-review-head-user-info-name" ng-show="review.anonym">Некто</div>
						<div class="lo-r-cont-review-head-user-info-date">{{calcTime(review.date)}}</div>
					</div>
					<a class="lo-r-cont-review-head-user-edit" ng-show="can(place)" href="/#/places/{{place.properties.id}}/edit_review/{{review.id}}">
